{{log "character-sheet" this}}
<form class="{{cssClass}} {{actor.type}} flexcol" autocomplete="off">
  {{!-- Sheet Header --}}
  <header class="sheet-header">
    {{> "systems/insmv/templates/actor/parts/actor-identity.hbs"}}
  </header>
  <section class="sheet-body">
    {{> "systems/insmv/templates/actor/parts/actor-attributes.hbs"}}

    {{!-- Sheet Tab Navigation --}}
    <nav class="sheet-tabs tabs" data-group="mainTabs">
      {{!-- Default tab is specified in actor-sheet.mjs --}}
      <a class="item" data-tab="skills">{{localize "INS_MV.Tabs.Skills"}}</a>
      <a class="item" data-tab="wounds">{{localize "INS_MV.Tabs.Wounds"}}</a>
      <a class="item" data-tab="items">{{localize "INS_MV.Tabs.Items"}}</a>
      <a class="item" data-tab="powers">{{localize "INS_MV.Tabs.Powers"}}</a>
    </nav>

    {{log "@root.system.spe" @root.system.spe}}
    <div class="tab" data-tab="skills" data-group="mainTabs">
      <section class="grid grid-12col">
          <div class="items-header grid-span-12"><p>{{localize "INS_MV.Skills.Main"}}</p></div>
          {{#each this.config.talents.main as |attributes key|}}
            {{> "systems/insmv/templates/actor/parts/actor-skills.hbs" attributes=attributes  key=key }}
          {{/each}}
          
          <div class="items-header grid-span-12"><p>{{localize "INS_MV.Skills.Secondary"}}</p></div>
          {{#each this.config.talents.secondary as |attributes key|}}
            {{> "systems/insmv/templates/actor/parts/actor-skills.hbs" attributes=attributes  key=key}}
          {{/each}}

          <div class="items-header grid-span-12"><p>{{localize "INS_MV.Skills.Exotic"}}</p></div>
          {{#each this.config.talents.exotic as |attributes key|}}
            {{#if (or (lookup @root.options "edition") (lookup @root.system.exotic (concat key '_label')))}}
              {{> "systems/insmv/templates/actor/parts/actor-skills.hbs" attributes=attributes  key=key }}
            {{/if}} 
          {{/each}}
      </section>
    </div>
    
    <div class="tab" data-tab="wounds" data-group="mainTabs">
      {{> "systems/insmv/templates/actor/parts/actor-wounds.hbs"}}
    </div>
    
    <div class="tab" data-tab="items" data-group="mainTabs">
      {{> "systems/insmv/templates/actor/parts/actor-items.hbs"}}
    </div>
    
    <div class="tab" data-tab="powers" data-group="mainTabs">
      {{> "systems/insmv/templates/actor/parts/actor-powers.hbs"}}
    </div>
  </section>
  



</form>
